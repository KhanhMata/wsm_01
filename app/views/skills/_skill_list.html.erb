<div class="banner-w3agile">
  <div class="row">
    <div class="col-md-12">
      <%= link_to t(".new_skill"), new_skill_path, remote: true,
        class: "btn btn-primary" %>
    </div>
  </div>
  <div class="container">
    <h3>
      <span>
        <h1><%= t ".title" %></h1>
      </span>
    </h3>
  </div>
</div>
<div class="containe">
  <div class="col-md-12">
    <table  id="table_list" class="table table-striped table-bordered">
      <tr>
        <th class="text-center"><%= t ".index_num" %></th>
        <th class="text-center"><%= t ".name" %></th>
        <th class="text-center" colspan="2"><%= t ".actions" %></th>
      </tr>
      <tbody>
        <%= render "skill_item" %>
      </tbody>
    </table>
    <%= paginate @skills %>
  </div>
  <div id="level-modal" class="modal fade"></div>
</div>
