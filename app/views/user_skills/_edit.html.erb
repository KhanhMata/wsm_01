<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <h1><%= t ".title" %></h1>
    </div>
    <div class="women_main">
      <div class="row">
        <div class="agiler">
          <div class="main">
            <div class="edit-form">
              <%= simple_form_for @user_skill, remote: true do |f| %>
                <div class="modal-body">
                  <div class="form_errors">
                    <%= render "shared/error_messages",
                      object: @user_skill %>
                  </div>
                  <div class="form-group">
                    <%= f.label :user_id %>
                    <%= f.select :user_id,
                      @users.collect {|user| [user.name, user.id]}, {},
                        {class: "form-control", disabled: true} %>
                  </div>
                  <div class="form-group">
                    <%= f.label :skill_id %>
                    <%= f.select :skill_id,
                      @skills.collect {|skill| [skill.name, skill.id]}, {},
                        {class: "form-control", disabled: true} %>
                  </div>
                  <div class="form-group">
                    <%= f.label :experience_years %>
                    <%= f.text_field :experience_years, class: "form-control" %>
                  </div>
                </div>
                <div class="modal-footer">
                  <span class="pull-right">
                    <%= f.submit t(".submit_button"),
                      class: "btn btn-primary" %>
                  </span>
                  <span class="pull-left">
                    <%= link_to t(".cancel_button"), skills_path,
                      class: "btn btn-warning", data: {dismiss: "modal"} %>
                  </span>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
