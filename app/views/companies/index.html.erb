<div class="row">
  <div class="col-md-10 col-md-offset-1">
    <h2><%= t ".company_list" %></h2>
    <table class="table table-hover table-bordered">
      <thead>
        <tr>
          <th><%= t ".index_num" %></th>
          <th><%= t ".name" %></th>
          <th><%= t ".number_of_staffs" %></th>
          <th><%= t ".number_of_workspaces" %></th>
          <th><%= t ".action" %></th>
        </tr>
      </thead>
      <tbody id = "body_table_data">
        <%= render "list_companies" %>
      </tbody>
    </table>
    <%= paginate @companies %>
  </div>
  <div id="level-modal" class="modal fade"></div>
</div>
